{
  "name": "autohub-backend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/backend/src/index.js",
    "verify:endpoints": "tsx scripts/verifyEndpoints.ts",
    "test:email-subscription": "npm run build && tsx scripts/testEmailSubscription.ts",
    "test:unsubscribe-repeat": "npm run build && tsx scripts/testUnsubscribeRepeatFlow.ts",
    "test:site-validation": "npm run build && tsx scripts/testSiteValidation.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/multer": "^1.4.12",
    "@types/nodemailer": "^6.4.15",
    "@types/node": "^22.8.1",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  }
}
